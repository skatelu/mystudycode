        <col_item field="LINES_INPUT" title="输入行数" width="100" type="input" sortable="false"/>
        <col_item field="LINES_OUTPUT" title="输出行数" width="100" type="input" sortable="false"/>
        <col_item field="ERRORS" title="错误次数" width="100" type="input" sortable="false"/>
        <col_item field="ENDDATE" title="开始时间" width="100" type="input" sortable="false"/>
        <col_item field="RUNTIME" title="耗时" width="100" type="input" sortable="false"/>
        <col_item field="LOGDATE" title="结束时间" width="100" type="input" sortable="false"/>
        <col_item field="LOG_FIELD" title="日志详情" width="100" type="input" sortable="false"/>
    </view>
